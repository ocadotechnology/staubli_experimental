<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="start">
    <Code><![CDATA[begin
  userPage()
  cls()
  
  // feedback task will run every 32 ms (approx. 31 HZ)
  //nFbkPeriod=0.032
  nFbkPeriod=0.05
  // motion task will run every 12 ms (approx. 83 Hz)
  nMotionPeriod=0.012
  
  nDeg2Rad = 3.14159265359 / 180.0

  sFbkTaskName="publisher"

  // initialise TCP/IP parameters
  //call initParams()

  // create feedback (state and status) task to send data to industrial_robot)client
  taskCreateSync sFbkTaskName,nFbkPeriod,bFbkOverrun,jStateStreamer()

end]]></Code>
  </Program>
</Programs>