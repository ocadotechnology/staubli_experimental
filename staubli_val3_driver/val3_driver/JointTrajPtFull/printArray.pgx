<?xml version="1.0" encoding="utf-8" ?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2" >
  <Program name="printArray" access="private" >
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1" >
      <Parameter name="x_sFormat" type="string" use="value" xsi:type="element" dimensions="1" />
      <Parameter name="x_nArray" type="num" use="reference" xsi:type="array" dimensions="1" />
    </Parameters>
    <Locals>
      <Local name="i" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  for i=0 to size(x_nArray)-2
    put(toString(x_sFormat,x_nArray[i]))
    put(" ")
  endFor
  putln(toString(x_sFormat,x_nArray[size(x_nArray)-1]))
end]]></Code>
  </Program>
</Programs>