<?xml version="1.0" encoding="utf-8"?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2">
  <Program name="decodeJointData" access="public">
    <Parameters xmlns="http://www.staubli.com/robotics/VAL3/Param/1">
      <Parameter name="x_nStatus" type="num" xsi:type="element" use="reference" />
      <Parameter name="x_nJointData" type="num" xsi:type="array" use="reference" />
      <Parameter name="x_nDWordBody" type="num" xsi:type="array" use="reference" dimensions="2" />
      <Parameter name="x_nRowIndex" type="num" xsi:type="element" />
    </Parameters>
    <Locals>
      <Local name="i" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  if x_nStatus < 0
    return
  endIf
  x_nStatus = 0
  for i = 0 to size(x_nJointData) - 1
    call decodeShareReal(x_nStatus, x_nJointData[i], x_nDWordBody, x_nRowIndex + i)
    if x_nStatus < 0
      putln("Failed to decode joint data at index: "+toString("", x_nRowIndex + i))
      return
    endIf    
  endFor
end]]></Code>
  </Program>
</Programs>