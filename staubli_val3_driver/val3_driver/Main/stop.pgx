<?xml version="1.0" encoding="utf-8" ?>
<Programs xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.staubli.com/robotics/VAL3/Program/2" >
  <Program name="stop" access="private" >
    <Locals>
      <Local name="l_nI" type="num" xsi:type="array" size="1" />
    </Locals>
    <Code><![CDATA[begin
  putln("TaskStatus: "+toString("",taskStatus(sInputBuffering)))
  bInputBuffering=false
  bTaskManager=false
  resetMotion()
  //wait(taskStatus(sInputBuffering)==0)
  delay(2)
  if taskStatus(sInputBuffering)!=0
    taskKill(sInputBuffering)
  endIf
  putln("TaskStatus: "+toString("",taskStatus(sInputBuffering)))
  putln("Stopped.")
end]]></Code>
  </Program>
</Programs>